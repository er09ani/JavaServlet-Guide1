<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>JavaServlet-Guide1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>abc</servlet-name>
    <servlet-class>com.plexus.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>pqr</servlet-name>
    <servlet-class>com.plexus.SqServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pqr</servlet-name>
    <url-pattern>/sq</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>pqr1</servlet-name>
    <servlet-class>com.plexus.SqServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>pqr1</servlet-name>
    <url-pattern>/sq1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>abc1</servlet-name>
    <servlet-class>com.plexus.AddServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc1</servlet-name>
    <url-pattern>/add1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>abc2</servlet-name>
    <servlet-class>com.plexus.AddServlet2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc2</servlet-name>
    <url-pattern>/add2</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.plexus.LoginServlet</servlet-class>
    <init-param>
      <param-name>databaseUserName</param-name>
      <param-value>Enter Database username here</param-value>
    </init-param>
    <init-param>
      <param-name>databaseUserPassword</param-name>
      <param-value>Enter Database user password here</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>databaseUrl</param-name>
    <param-value>Enter JDBC url here</param-value>
  </context-param>
</web-app>